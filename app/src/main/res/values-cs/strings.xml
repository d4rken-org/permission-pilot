    <string name="permissions_details_description_restrictions_caveat_description">Aplikace nainstalované v jiných uživatelských profilech jsou z výše uvedených výpočtů vyloučeny kvůli omezením systému Android.</string>
    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Umožňuje aplikacím komunikovat se štítky, kartami a čtečkami NFC (Near Field Communication).</string>
    <string name="permission_manage_media_label">Správa médií</string>
    <string name="permission_manage_media_description">Umožňuje aplikacím upravovat nebo odstraňovat mediální soubory vytvořené jinými aplikacemi, aniž by se vás zeptaly. Aplikace musí mít oprávnění k přístupu k souborům a médiím.</string>
    <string name="permission_usage_data_access_label">Přístup k využití dat</string>
    <string name="permission_usage_data_access_description">Umožňuje aplikacím sledovat, jaké další aplikace používáte a jak často, identifikovat poskytovatele služeb, nastavení jazyka a další údaje o používání.</string>
    <string name="permission_modify_system_settings_label">Správa systémových nastavení</string>
    <string name="permission_modify_system_settings_description">Umožňuje aplikacím měnit údaje o nastavení systému. Škodlivé aplikace mohou poškodit konfiguraci systému.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth</string>
    <string name="permission_bluetooth_advertise_description">Umožňuje aplikacím přistupovat na blízká zařízení Bluetooth.</string>
    <string name="permission_get_accounts_label">Získávání účtů</string>
    <string name="permission_get_accounts_description">Umožňuje aplikacím získat seznam účtů, které telefon zná. To může zahrnovat všechny účty vytvořené nainstalovanými aplikacemi.</string>
    <string name="permission_access_background_location_label">Přístup k poloze na pozadí</string>
    <string name="permission_access_background_location_description">Umožňuje aplikacím kdykoli přistupovat k poloze, i když nejsou používány.</string>
    <string name="permission_access_media_location_label">Přístup k umístění médií</string>
    <string name="permission_access_media_location_description">Umožňuje aplikacím určovat uložení vaší sbírky médií.</string>
    <string name="permission_manage_accounts_label">Správa účtů</string>
    <string name="permission_manage_accounts_description">Umožňuje aplikacím spravovat účty v zařízení.</string>
    <string name="permission_wifi_control_label">Ovládání Wi-Fi</string>
    <string name="permission_wifi_control_description">Umožňuje aplikacím zapínat a vypínat Wi-Fi, skenovat sítě Wi-Fi a připojovat se k nim, přidávat a odebírat sítě nebo spouštět pouze místní HotSpot.</string>
    <string name="permission_change_network_state_label">Změna stavu sítě</string>
    <string name="permission_change_network_state_description">Umožňuje aplikacím měnit stav síťového připojení.</string>
    <string name="permission_read_sync_settings_label">Čtení nastavení synchronizace</string>
    <string name="permission_read_sync_settings_description">Umožňuje aplikacím číst nastavení synchronizace účtu.</string>
    <string name="permission_install_unknown_apps_label">Instalace neznámých aplikací (z neznámých zdrojů)</string>
    <string name="permission_install_unknown_apps_description">Umožňuje aplikacím instalovat jiné aplikace do zařízení (sideload). To může ohrozit vaše zařízení a data.</string>
    <string name="permission_all_files_access_label">Přístup ke všem souborům</string>
    <string name="permission_all_files_access_description">Umožňuje aplikacím číst, upravovat a odstraňovat všechny soubory v zařízení i v připojených úložných zařízeních. Aplikace s tímto oprávněním mohou přistupovat i k souborům, které s aplikací nesouvisejí, aniž by vás na to upozornily.</string>
    <string name="permission_appear_on_top_label">Zobrazení přes ostatní aplikace</string>
    <string name="permission_appear_on_top_description">Umožňuje aplikacím zobrazovat věci nad ostatními používanými aplikacemi (včetně zobrazování reklam). To může narušit používání jiných aplikací.</string>
    <string name="permission_files_and_media_label">Soubory a média</string>
    <string name="permission_files_and_media_description">Umožňuje aplikacím přístup k souborům a médiím v zařízení.</string>
    <string name="permission_access_to_media_only_label">Přístup pouze k médiím</string>
    <string name="permission_access_to_media_only_description">Umožňuje aplikacím přístup pouze k médiím v zařízení.</string>
    <string name="permission_access_precise_location_label">Přístup k přesné poloze</string>
    <string name="permission_access_precise_location_description">Umožňuje aplikacím přístup k přesné poloze zařízení. To se u většiny aplikací NEDOPORUČUJE (s výjimkou některých aplikací, jako jsou mapy, taxislužby, služby pro sledování zařízení, například Find My Device atd.).</string>
    <string name="permission_access_approximate_location_label">Přístup k přibližné poloze</string>
    <string name="permission_access_approximate_location_description">Umožňuje aplikacím přístup k přibližné poloze zařízení. Toto oprávnění je dostačující pro většinu aplikací, které potřebují přístup k vaší poloze pro základní funkce.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorování optimalizací baterie</string>
    <string name="permission_ignore_battery_optimizations_description">Umožňuje nerušený běh aplikací na pozadí. To může vybíjet baterii. Doporučuje se pouze u aplikací, které provádějí průběžné/časté zálohování a/nebo synchronizaci (např. Fotky Google), u velmi často používaných aplikací (např. e-mail, chat, sms atd.) a také u aplikací, které mají trvalé oznámení.</string>
    <string name="permission_notification_access_label">Přístup k oznámením</string>
    <string name="permission_notification_access_description">Umožňuje aplikacím číst všechna oznámení včetně osobních informací (například jména kontaktů a text přijatých zpráv), odložení nebo zamítnutí oznámení nebo spuštění akčních tlačítek, která obsahují (například přijetí telefonního hovoru, vymazání sms/e-mailu atd.), zapnutí nebo vypnutí funkce Nerušit (DND) a změnu souvisejících nastavení.</string>
    <string name="permission_do_not_disturb_permission_label">Oprávnění k režimu Nerušit(DND)</string>
    <string name="permission_do_not_disturb_permission_description">Umožňuje aplikacím zapnout nebo vypnout funkci Nerušit (DND) a změnit související nastavení.</string>
    <string name="permission_alarms_and_reminders_label">Budíky a připomenutí</string>
    <string name="permission_alarms_and_reminders_description">Umožňuje aplikacím nastavovat budíky a plánovat časově závislé akce. Díky tomu mohou aplikace běžet na pozadí, což může spotřebovávat více baterie. Vypnutí tohoto oprávnění pro aplikaci způsobí, že stávající budíky a časové události naplánované danou aplikací přestanou fungovat.</string>
    <string name="permission_premium_sms_services_label">Přístup k prémiovým SMA</string>
    <string name="permission_premium_sms_services_description">Umožňuje aplikacím používat prémiové služby textových zpráv. To vás bude stát peníze.</string>
    <string name="permission_unlimited_data_access_label">Neomezený přístup k datům</string>
    <string name="permission_unlimited_data_access_description">Umožňuje aplikacím přístup k neomezeným datům. Aplikace mohou spotřebovávat data, i když je zapnutý Spořič dat.</string>
    <string name="permission_group_calendar_label">Kalendář</string>
    <string name="permission_group_calendar_description"><![CDATA[Oprávnění související s kalendářem, např. zobrazení a úprava záznamů.]]></string>
    <string name="permission_group_files_label">Soubory a média</string>
    <string name="permission_group_files_description">Oprávnění související se soubory. Přístup k úložišti, dokumentům a médiím.</string>
    <string name="permission_group_other_label">Ostatní</string>
    <string name="permission_group_other_description">Ostatní oprávnění bez konkrétní kategorie.</string>
    <string name="permission_group_messaging_label">Zprávy</string>
    <string name="permission_group_messaging_description">SMS, MMS a další. Vše, co souvisí se zasíláním zpráv.</string>
    <string name="permission_group_connectivity_label">Konektivity</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth a další. Oprávnění související s komunikací s okolním světem.</string>
    <string name="permission_group_sensors_label">Senzory</string>
    <string name="permission_group_sensors_description">Akcelerometr, gyroskop, tělesné senzory, rozpoznávání aktivity a další. Způsoby, kterými může zařízení snímat vstupní údaje.</string>
    <string name="permission_group_calls_label">Volání</string>
    <string name="permission_group_calls_description">Oprávnění související s voláním.</string>
    <string name="permission_group_location_label">Poloha</string>
    <string name="permission_group_location_description">Oprávnění související s polohou.</string>
    <string name="permission_group_contacts_label">Kontakty</string>
    <string name="permission_group_contacts_description">"Čtení a zápis kontaktů."</string>
    <string name="permission_group_apps_label">Aplikace</string>
    <string name="permission_group_apps_description">Zvláštní přístupová oprávnění vyžadující výslovný souhlas uživatele.</string>
    <string name="permission_group_battery_label">Baterie</string>
    <string name="permission_group_battery_description">Oprávnění související s baterií.</string>
    <string name="permission_group_camera_label">Fotoaparát</string>
    <string name="permission_group_camera_description">Oprávnění související s fotoaparátem.</string>
