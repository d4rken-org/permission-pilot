    <string name="permissions_details_description_restrictions_caveat_description">Aplikacije instalirane u drugim korisničkim profilima su isključene iz gornjeg izračuna zbog ograničenja Androida.</string>
    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Omogućuje aplikacijama komunikaciju s Near Field Communication (NFC) oznakama, karticama i čitačima.</string>
    <string name="permission_manage_media_label">Upravljanje medijima</string>
    <string name="permission_manage_media_description">Omogućuje aplikacijama mijenjanje ili brisanje medijskih datoteka stvorenih s drugim aplikacijama BEZ pitanja. Aplikacije moraju imati dozvolu za pristup datotekama i medijima.</string>
    <string name="permission_usage_data_access_label">Pristup podacima o korištenju</string>
    <string name="permission_usage_data_access_description">Omogućuje aplikacijama praćenje koje druge aplikacije koristite i koliko često, identificiranje vašeg pružatelja usluga, jezične postavke i druge podatke o korištenju.</string>
    <string name="permission_modify_system_settings_label">Modificiranje sistemskih postavki</string>
    <string name="permission_modify_system_settings_description">Omogućuje aplikacijama mijenjanje podataka sistemskih postavki. Zlonamjerne aplikacije mogu oštetiti konfiguraciju vašeg sustava.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth oglašavanje</string>
    <string name="permission_bluetooth_advertise_description">Omogućuje aplikacijama oglašavanje na obližnjim Bluetooth uređajima.</string>
    <string name="permission_get_accounts_label">Preuzmi račune</string>
    <string name="permission_get_accounts_description">Omogućuje aplikacijama preuzimanje popisa računa poznatih telefonu. To može uključivati sve račune stvorene aplikacijama koje ste instalirali.</string>
    <string name="permission_access_background_location_label">Pristup lokaciji u pozadini</string>
    <string name="permission_access_background_location_description">Omogućuje aplikacijama pristup lokaciji u bilo koje vrijeme, čak i kada se ne koriste.</string>
    <string name="permission_access_media_location_label">Pristup lokaciji medija</string>
    <string name="permission_access_media_location_description">Omogućuje aplikacijama čitanje lokacije iz vaše medijske kolekcije.</string>
    <string name="permission_manage_accounts_label">Upravljaj računima</string>
    <string name="permission_manage_accounts_description">Omogućuje aplikacijama upravljanje računima na uređaju.</string>
    <string name="permission_wifi_control_label">WiFi kontrola</string>
    <string name="permission_wifi_control_description">Omogućuje aplikacijama uključivanje i isključivanje WiFi-ja, skeniranje i povezivanje na WiFi mreže, dodavanje ili uklanjanje mreža ili pokretanje lokalnog HotSpot-a.</string>
    <string name="permission_change_network_state_label">Promjena stanja mreže</string>
    <string name="permission_change_network_state_description">Omogućuje aplikacijama promjenu stanja mrežne povezanosti.</string>
    <string name="permission_read_sync_settings_label">Čitanje postavki sinkronizacije</string>
    <string name="permission_read_sync_settings_description">Omogućuje aplikacijama čitanje postavki sinkronizacije za račun.</string>
    <string name="permission_install_unknown_apps_label">Instaliraj nepoznate aplikacije (Nepoznati izvori)</string>
    <string name="permission_install_unknown_apps_description">Omogućuje aplikacijama instaliranje (sideload) drugih aplikacija na vaš uređaj. To može ugroziti vaš uređaj i podatke.</string>
    <string name="permission_all_files_access_label">Pristup svim datotekama</string>
    <string name="permission_all_files_access_description">Omogućuje aplikacijama čitanje, mijenjanje i brisanje svih datoteka na vašem uređaju kao i svih povezanih uređaja za pohranu. Aplikacije s ovom dozvolom također mogu pristupiti datotekama koje nisu povezane s aplikacijom, BEZ obavještavanja.</string>
    <string name="permission_appear_on_top_label">Prikaži se na vrhu</string>
    <string name="permission_appear_on_top_description">Omogućuje aplikacijama prikazivanje stvari na vrhu drugih aplikacija koje koristite (uključujući prikazivanje reklama). To može ometati korištenje drugih aplikacija.</string>
    <string name="permission_files_and_media_label">Datoteke i mediji</string>
    <string name="permission_files_and_media_description">Omogućuje aplikacijama pristup datotekama i medijima na vašem uređaju.</string>
    <string name="permission_access_to_media_only_label">Pristup samo medijima</string>
    <string name="permission_access_to_media_only_description">Omogućuje aplikacijama pristup samo medijima na vašem uređaju.</string>
    <string name="permission_access_precise_location_label">Pristup preciznoj/točnoj lokaciji</string>
    <string name="permission_access_precise_location_description">Omogućuje aplikacijama pristup preciznoj/točnoj lokaciji vašeg uređaja. To se NE preporučuje za većinu aplikacija (osim za neke aplikacije poput karata, taksi usluga, usluga praćenja uređaja poput Find My Device itd.).</string>
    <string name="permission_access_approximate_location_label">Pristup približnoj lokaciji</string>
    <string name="permission_access_approximate_location_description">Omogućuje aplikacijama pristup približnoj lokaciji vašeg uređaja. Ova dozvola je dovoljna za većinu aplikacija koje trebaju pristup vašoj lokaciji za osnovnu funkcionalnost.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignoriraj optimizacije baterije</string>
    <string name="permission_ignore_battery_optimizations_description">Omogućuje aplikacijama nesmetano rad u pozadini. To može trošiti bateriju. Preporučuje se samo za aplikacije koje izvršavaju kontinuirane/česte sigurnosne kopije i/ili sinkronizacije (npr. Google Photos), vrlo često korištene aplikacije (npr. email, chat, SMS itd.), kao i za aplikacije koje imaju trajnu obavijest.</string>
    <string name="permission_notification_access_label">Pristup obavijestima</string>
    <string name="permission_notification_access_description">Omogućuje aplikacijama čitanje svih vaših obavijesti, uključujući osobne informacije (poput imena kontakata i teksta poruka koje primate), odgađanje ili odbacivanje obavijesti ili pokretanje gumbova za radnje koje sadrže (poput odgovaranja na telefonske pozive, brisanje SMS/email poruka itd.) i uključivanje ili isključivanje načina Ne ometaj (DND) i mijenjanje povezanih postavki.</string>
    <string name="permission_do_not_disturb_permission_label">Dozvola za Ne ometaj (DND)</string>
    <string name="permission_do_not_disturb_permission_description">Omogućuje aplikacijama uključivanje ili isključivanje načina Ne ometaj (DND) i mijenjanje povezanih postavki.</string>
    <string name="permission_alarms_and_reminders_label">Alarmi i podsjetnici</string>
    <string name="permission_alarms_and_reminders_description">Omogućuje aplikacijama postavljanje alarma i planiranje vremensko-osjetljivih radnji. To omogućuje aplikacijama rad u pozadini, što može trošiti više baterije. Isključivanje ove dozvole za aplikaciju bi dovelo do prestanka rada postojećih alarma i vremenski-utemeljenih događaja zakazanih od te određene aplikacije.</string>
    <string name="permission_premium_sms_services_label">Pristup premium SMS-u</string>
    <string name="permission_premium_sms_services_description">Omogućuje aplikacijama korištenje premium usluga tekstualnih poruka. To će vas koštati novac.</string>
    <string name="permission_unlimited_data_access_label">Neograničen pristup podacima</string>
    <string name="permission_unlimited_data_access_description">Omogućuje aplikacijama pristup neograničenim podacima. Aplikacije mogu trošiti podatke čak i kada je uključen Štednjak podataka.</string>
    <string name="permission_group_calendar_label">Kalendar</string>
    <string name="permission_group_calendar_description"><![CDATA[Dozvole povezane s kalendarom, npr. pregled i uređivanje unosa.]]></string>
    <string name="permission_group_files_label">Datoteke i mediji</string>
    <string name="permission_group_files_description">Dozvole povezane s datotekama. Pristup pohrani, dokumentima i medijima.</string>
    <string name="permission_group_other_label">Ostalo</string>
    <string name="permission_group_other_description">Ostale dozvole bez posebne kategorije.</string>
    <string name="permission_group_messaging_label">Poruke</string>
    <string name="permission_group_messaging_description">SMS, MMS i više. Sve povezano s porukama.</string>
    <string name="permission_group_connectivity_label">Povezivost</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth i više. Dozvole povezane s komunikacijom s vanjskim svijetom.</string>
    <string name="permission_group_sensors_label">Senzori</string>
    <string name="permission_group_sensors_description">Akcelerometar, žiroskop, tjelesni senzori, prepoznavanje aktivnosti i više. Načini na koje vaš uređaj može uhvatiti unos.</string>
