    <string name="permissions_details_description_restrictions_caveat_description">Apper installert i andre brukerprofiler er ekskludert fra beregningen ovenfor på grunn av Android-begrensninger.</string>
    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Lar apper kommunisere med Near Field Communication (NFC) tagger, kort og lesere.</string>
    <string name="permission_manage_media_label">Administrer media</string>
    <string name="permission_manage_media_description">Lar apper endre eller slette mediefiler opprettet med andre apper UTEN å spørre deg. Apper må ha tillatelse til å få tilgang til filer og media.</string>
    <string name="permission_usage_data_access_label">Tilgang til bruksdata</string>
    <string name="permission_usage_data_access_description">Lar apper spore hvilke andre apper du bruker og hvor ofte, identifisere tjenesteleverandøren din, språkinnstillinger og andre bruksdata.</string>
    <string name="permission_modify_system_settings_label">Endre systeminnstillinger</string>
    <string name="permission_modify_system_settings_description">Lar apper endre systemets innstillingsdata. Skadelige apper kan korruptere systemets konfigurasjon.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth-annonsering</string>
    <string name="permission_bluetooth_advertise_description">Lar apper annonsere til nærliggende Bluetooth-enheter.</string>
    <string name="permission_get_accounts_label">Hent kontoer</string>
    <string name="permission_get_accounts_description">Lar apper hente listen over kontoer kjent av telefonen. Dette kan inkludere alle kontoer opprettet av applikasjoner du har installert.</string>
    <string name="permission_access_background_location_label">Bakgrunnsposisjonstilgang</string>
    <string name="permission_access_background_location_description">Lar apper få tilgang til posisjon når som helst, selv når de ikke er i bruk.</string>
    <string name="permission_access_media_location_label">Mediaposisjonstilgang</string>
    <string name="permission_access_media_location_description">Lar apper lese posisjon fra mediasamlingen din.</string>
    <string name="permission_manage_accounts_label">Administrer kontoer</string>
    <string name="permission_manage_accounts_description">Lar apper administrere kontoer på enheten.</string>
    <string name="permission_wifi_control_label">WiFi-kontroll</string>
    <string name="permission_wifi_control_description">Lar apper slå WiFi på eller av, skanne og koble til WiFi-nettverk, legge til eller fjerne nettverk, eller starte en lokal-bare HotSpot.</string>
    <string name="permission_change_network_state_label">Endre nettverkstilstand</string>
    <string name="permission_change_network_state_description">Lar apper endre tilstanden til nettverkstilkobling.</string>
    <string name="permission_read_sync_settings_label">Les synkroniseringsinnstillinger</string>
    <string name="permission_read_sync_settings_description">Lar apper lese synkroniseringsinnstillinger for en konto.</string>
    <string name="permission_install_unknown_apps_label">Installer ukjente apper (Ukjente kilder)</string>
    <string name="permission_install_unknown_apps_description">Lar apper installere (sidelaste) andre apper på enheten din. Dette kan sette enheten og dataene dine i fare.</string>
    <string name="permission_all_files_access_label">Tilgang til alle filer</string>
    <string name="permission_all_files_access_description">Lar apper lese, endre og slette alle filer på enheten din samt alle tilkoblede lagringsenheter. Apper med denne tillatelsen kan også få tilgang til filer som ikke er relatert til appen, UTEN å varsle deg.</string>
    <string name="permission_appear_on_top_label">Vises på toppen</string>
    <string name="permission_appear_on_top_description">Lar apper vise ting på toppen av andre apper du bruker (inkludert å vise annonser). Dette kan forstyrre bruken av andre apper.</string>
    <string name="permission_files_and_media_label">Filer og media</string>
    <string name="permission_files_and_media_description">Lar apper få tilgang til filer og media på enheten din.</string>
    <string name="permission_access_to_media_only_label">Tilgang kun til media</string>
    <string name="permission_access_to_media_only_description">Lar apper få tilgang kun til media på enheten din.</string>
    <string name="permission_access_precise_location_label">Tilgang til nøyaktig/presis posisjon</string>
    <string name="permission_access_precise_location_description">Lar apper få tilgang til enhetens nøyaktige/presise posisjon. Dette anbefales IKKE for de fleste apper (unntatt for noen apper som kart, taxitjenester, enhetsporingsstjenester som Find My Device osv.).</string>
    <string name="permission_access_approximate_location_label">Tilgang til omtrentlig posisjon</string>
    <string name="permission_access_approximate_location_description">Lar apper få tilgang til enhetens omtrentlige posisjon. Denne tillatelsen er tilstrekkelig for de fleste apper som trenger tilgang til posisjonen din for kjernefunksjonalitet.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorer batterioptimering</string>
    <string name="permission_ignore_battery_optimizations_description">Lar apper kjøre uhindret i bakgrunnen. Dette kan tømme batteriet. Anbefales kun for apper som utfører kontinuerlige/hyppige sikkerhetskopier og/eller synkroniseringer (f.eks. Google Bilder), svært hyppig brukte apper (f.eks. e-post, chat, SMS, osv.), samt for de appene som har en vedvarende varsling.</string>
    <string name="permission_notification_access_label">Varslingstilgang</string>
    <string name="permission_notification_access_description">Lar apper lese alle varslene dine, inkludert personlig informasjon (som kontaktnavn og tekst fra meldinger du mottar), utsette eller avvise varsler eller utløse handlingsknapper de inneholder (som å svare på telefonsamtaler, slette SMS/e-post, osv.) og slå på eller av Ikke forstyrr (DND) og endre relaterte innstillinger.</string>
    <string name="permission_do_not_disturb_permission_label">Ikke forstyrr (DND) tillatelse</string>
    <string name="permission_do_not_disturb_permission_description">Lar apper slå på eller av Ikke forstyrr (DND) og endre relaterte innstillinger.</string>
    <string name="permission_alarms_and_reminders_label">Alarmer og påminnelser</string>
    <string name="permission_alarms_and_reminders_description">Lar apper sette alarmer og planlegge tidssensitive handlinger. Dette lar apper kjøre i bakgrunnen, som kan bruke mer batteri. Å slå av denne tillatelsen for en app ville føre til at eksisterende alarmer og tidsbaserte hendelser planlagt av den spesifikke appen sluttet å fungere.</string>
    <string name="permission_premium_sms_services_label">Premium SMS-tilgang</string>
    <string name="permission_premium_sms_services_description">Lar apper bruke premium tekstmeldingstjenester. Dette vil koste deg penger.</string>
    <string name="permission_unlimited_data_access_label">Ubegrenset datatilgang</string>
    <string name="permission_unlimited_data_access_description">Lar apper få tilgang til ubegrenset data. Apper kan forbruke data selv når Datasparer er PÅ.</string>
    <string name="permission_group_calendar_label">Kalender</string>
    <string name="permission_group_calendar_description"><![CDATA[Kalenderrelaterte tillatelser, f.eks. vis & rediger oppføringer.]]></string>
