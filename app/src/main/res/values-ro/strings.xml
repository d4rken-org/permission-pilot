    <string name="permission_write_calendar_label">Scrie în calendar</string>
    <string name="permission_write_calendar_description">Permite adăugarea, eliminarea și modificarea evenimentelor de calendar pe dispozitivul tău.</string>
    <string name="permission_read_calendar_label">Citește calendarul</string>
    <string name="permission_read_calendar_description">Permite citirea evenimentelor de calendar stocate pe dispozitivul tău.</string>
    <string name="permission_write_contacts_label">Modifică contactele</string>
    <string name="permission_write_contacts_description">Permite modificarea datelor de contact stocate pe dispozitivul tău.</string>
    <string name="permission_read_contacts_label">Citește contactele</string>
    <string name="permission_read_contacts_description">Permite citirea datelor de contact stocate pe dispozitivul tău.</string>
    <string name="permission_get_accounts_label">Găsește conturile</string>
    <string name="permission_get_accounts_description">Permite aplicațiilor să obțină lista conturilor cunoscute de telefon. Aceasta poate include conturile create de aplicațiile instalate.</string>
    <string name="permission_access_background_location_label">Acces la locația din fundal</string>
    <string name="permission_access_background_location_description">Permite aplicațiilor să acceseze locația oricând, chiar când nu sunt în uz.</string>
    <string name="permission_access_media_location_label">Acces la locația media</string>
    <string name="permission_access_media_location_description">Permite aplicațiilor să citească informațiile de locație din colecția ta media.</string>
    <string name="permission_manage_accounts_label">Gestionează conturile</string>
    <string name="permission_manage_accounts_description">Permite aplicațiilor să gestioneze conturile de pe dispozitiv.</string>
    <string name="permission_wifi_control_label">Control WiFi</string>
    <string name="permission_wifi_control_description">Permite aplicațiilor să activeze sau dezactiveze WiFi, să scaneze și să se conecteze la rețele WiFi, să adauge sau să elimine rețele sau să pornească un hotspot local.</string>
    <string name="permission_change_network_state_label">Modifică starea rețelei</string>
    <string name="permission_change_network_state_description">Permite aplicațiilor să modifice starea conectivității rețelei.</string>
    <string name="permission_read_sync_settings_label">Citește setările de sincronizare</string>
    <string name="permission_read_sync_settings_description">Permite aplicațiilor să citească setările de sincronizare pentru un cont.</string>
    <string name="permission_install_unknown_apps_label">Instalează aplicații necunoscute (Surse necunoscute)</string>
    <string name="permission_install_unknown_apps_description">Permite aplicațiilor să instaleze (sideload) alte aplicații pe dispozitiv. Aceasta poate pune în pericol dispozitivul și datele.</string>
    <string name="permission_all_files_access_label">Acces la toate fișierele</string>
    <string name="permission_all_files_access_description">Permite aplicațiilor să citească, modifice și să șteargă toate fișierele de pe dispozitiv și din orice stocare atașată. Aplicațiile cu această permisiune pot accesa fișiere nesolicitate FĂRĂ notificare.</string>
    <string name="permission_appear_on_top_label">Apare deasupra</string>
    <string name="permission_appear_on_top_description">Permite aplicațiilor să afișeze lucruri peste alte aplicații în uz (inclusiv afișarea reclamelor). Aceasta poate interfera cu folosirea altor aplicații.</string>
    <string name="permission_files_and_media_label">Fișiere și media</string>
    <string name="permission_files_and_media_description">Permite aplicațiilor să acceseze fișiere și media pe dispozitiv.</string>
    <string name="permission_access_to_media_only_label">Acces doar la media</string>
    <string name="permission_access_to_media_only_description">Permite aplicațiilor să acceseze doar media pe dispozitiv.</string>
    <string name="permission_access_precise_location_label">Acces la locația precisă</string>
    <string name="permission_access_precise_location_description">Permite aplicațiilor să folosească locația precisă a dispozitivului. Aceasta NU este recomandată pentru majoritatea aplicațiilor (cu excepția unor aplicații precum hărți, servicii de taxi, servicii de urmărire a dispozitivului precum Find My Device etc.).</string>
    <string name="permission_access_approximate_location_label">Acces la locația aproximativă</string>
    <string name="permission_access_approximate_location_description">Permite aplicațiilor să folosească locația aproximativă a dispozitivului. Această permisiune este suficientă pentru majoritatea aplicațiilor care au nevoie de locație pentru funcționalitățile de bază.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignoră optimizările bateriei</string>
    <string name="permission_ignore_battery_optimizations_description">Permite aplicațiilor să ruleze nerestricționat în fundal. Aceasta poate consuma bateria. Recomandat doar pentru aplicații care fac backup-uri/sincronizări continue/frecvente (de ex. Google Photos), aplicații foarte frecvent folosite (de ex. email, chat, SMS etc.) și aplicații care au notificări persistente.</string>
    <string name="permission_notification_access_label">Acces la notificări</string>
    <string name="permission_notification_access_description">Permite aplicațiilor să citească toate notificările, inclusiv informații personale (precum numele contactelor și textul mesajelor primite), să amâne sau să respingă notificări sau să declanșeze butoanele de acțiune (precum răspunderea la apeluri, ștergerea SMS/email-urilor etc.) și să activeze/dezactiveze modul Nu Deranja (DND) și să modifice setările asociate.</string>
    <string name="permission_do_not_disturb_permission_label">Permisiunea Nu Deranja (DND)</string>
    <string name="permission_do_not_disturb_permission_description">Permite aplicațiilor să activeze/dezactiveze modul Nu Deranja (DND) și să modifice setările asociate.</string>
    <string name="permission_alarms_and_reminders_label">Alarme și mementouri</string>
    <string name="permission_alarms_and_reminders_description">Permite aplicațiilor să seteze alarme și să programeze acțiuni sensibile la timp. Aceasta permite aplicațiilor să ruleze în fundal, ceea ce poate consuma mai multă baterie. Dezactivarea acestei permisiuni pentru o aplicație va cauza ca alarmele programate și evenimentele bazate pe timp să nu mai funcționeze.</string>
    <string name="permission_premium_sms_services_label">Acces la servicii SMS premium</string>
    <string name="permission_premium_sms_services_description">Permite aplicațiilor să folosească servicii de mesaje text premium. Aceasta te va costa bani.</string>
    <string name="permission_unlimited_data_access_label">Acces nelimitat la date</string>
    <string name="permission_unlimited_data_access_description">Permite aplicațiilor să acceseze date nelimitat. Aplicațiile pot consuma date chiar și când Economisorul de Date este ACTIVAT.</string>
    <string name="permission_group_calendar_label">Calendar</string>
    <string name="permission_group_calendar_description"><![CDATA[Permisiuni legate de calendar, de ex. vizualizarea și editarea intrărilor.]]></string>
    <string name="permission_group_files_label">Fișiere și media</string>
    <string name="permission_group_files_description">Permisiuni legate de fișiere. Acces la stocare, documente și media.</string>
    <string name="permission_group_other_label">Altele</string>
    <string name="permission_group_other_description">Alte permisiuni fără categorie specifică.</string>
    <string name="permission_group_messaging_label">Mesagerie</string>
    <string name="permission_group_messaging_description">SMS, MMS și altele. Tot ce ține de mesagerie.</string>
    <string name="permission_group_connectivity_label">Conectivitate</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth și altele. Permisiuni legate de comunicarea cu lumea exterioară.</string>
    <string name="permission_group_sensors_label">Senzori</string>
    <string name="permission_group_sensors_description">Accelerometru, giroscop, senzori corporali, recunoașterea activității și altele. Moduri prin care dispozitivul poate capta date.</string>
    <string name="permission_group_calls_label">Apeluri</string>
    <string name="permission_group_calls_description">Permisiuni legate de apeluri.</string>
    <string name="permission_group_location_label">Locație</string>
    <string name="permission_group_location_description">Permisiuni legate de locație.</string>
    <string name="permission_group_contacts_label">Contacte</string>
    <string name="permission_group_contacts_description">"Citirea și scrierea contactelor."</string>
    <string name="permission_group_apps_label">Aplicații</string>
    <string name="permission_group_apps_description">Permisiuni de acces special care necesită consimțământ explicit al utilizatorului.</string>
    <string name="permission_group_battery_label">Baterie</string>
    <string name="permission_group_battery_description">Permisiuni legate de baterie.</string>
    <string name="permission_group_camera_label">Cameră</string>
    <string name="permission_group_camera_description">Permisiuni legate de cameră.</string>
    <string name="permission_group_audio_label">Microfon</string>
    <string name="permission_group_audio_description">Permisiuni legate de microfon.</string>
    <string name="permission_picture_in_picture_label">Imagine în imagine</string>
    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Permite aplicațiilor să comunice cu etichete, carduri și cititoare NFC (Near Field Communication).</string>
    <string name="permission_manage_media_label">Gestionare media</string>
    <string name="permission_manage_media_description">Permite aplicațiilor să modifice sau să șteargă fișiere media create cu alte aplicații FĂRĂ a vă întreba. Aplicațiile trebuie să aibă permisiunea de a accesa fișiere și media.</string>
    <string name="permission_usage_data_access_label">Acces la datele de utilizare</string>
    <string name="permission_usage_data_access_description">Permite aplicațiilor să urmărească ce alte aplicații utilizați și cât de des, să identifice furnizorul de servicii, setările de limbă și alte date de utilizare.</string>
    <string name="permission_modify_system_settings_label">Modificare setări sistem</string>
    <string name="permission_modify_system_settings_description">Permite aplicațiilor să modifice datele de setări ale sistemului. Aplicațiile malițioase pot corupe configurația sistemului.</string>
    <string name="permission_bluetooth_advertise_label">Publicitate Bluetooth</string>
    <string name="permission_bluetooth_advertise_description">Permite aplicațiilor să facă publicitate către dispozitivele Bluetooth din apropiere.</string>
    <string name="permission_reboot_label">Repornire dispozitiv</string>
    <string name="permission_reboot_description">Permite aplicației să repornească dispozitivul. Disponibil doar pentru aplicațiile de sistem.</string>
    <string name="permission_bind_accessibility_service_label">Oferire serviciu de accesibilitate</string>
    <string name="permission_bind_accessibility_service_description">Permite aplicației să ofere un serviciu de accesibilitate care poate fi activat din setările sistemului.</string>

