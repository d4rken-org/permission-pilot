    <string name="permission_group_connectivity_label">Connettività</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth e altro. Permessi relativi alla comunicazione con il mondo esterno.</string>
    <string name="permission_group_sensors_label">Sensori</string>
    <string name="permission_group_sensors_description">Accelerometro, giroscopio, sensori corporei, riconoscimento attività e altro. Modi in cui il tuo dispositivo può catturare input.</string>
