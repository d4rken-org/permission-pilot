    <string name="permission_group_connectivity_label">Connectiviteit</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth en meer. Machtigingen gerelateerd aan communicatie met de buitenwereld.</string>
    <string name="permission_group_sensors_label">Sensoren</string>
    <string name="permission_group_sensors_description">Accelerometer, gyroscoop, lichaamssensoren, activiteitenherkenning en meer. Manieren waarop je apparaat invoer kan vastleggen.</string>
