    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Staat apps toe om te communiceren met Near Field Communication (NFC) tags, kaarten en lezers.</string>
    <string name="permission_manage_media_label">Media beheren</string>
    <string name="permission_manage_media_description">Staat apps toe om mediabestanden die zijn gemaakt met andere apps te wijzigen of verwijderen ZONDER het je te vragen. Apps moeten toestemming hebben om toegang te krijgen tot bestanden en media.</string>
    <string name="permission_usage_data_access_label">Toegang tot gebruiksgegevens</string>
    <string name="permission_usage_data_access_description">Staat apps toe om bij te houden welke andere apps je gebruikt en hoe vaak, je serviceprovider te identificeren, taalinstellingen en andere gebruiksgegevens.</string>
    <string name="permission_modify_system_settings_label">Systeeminstellingen wijzigen</string>
    <string name="permission_modify_system_settings_description">Staat apps toe om de gegevens van systeeminstellingen te wijzigen. Kwaadaardige apps kunnen de configuratie van je systeem beschadigen.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth adverteren</string>
    <string name="permission_bluetooth_advertise_description">Staat apps toe om te adverteren naar nabije Bluetooth-apparaten.</string>
    <string name="permission_get_accounts_label">Accounts ophalen</string>
    <string name="permission_get_accounts_description">Staat apps toe om de lijst van accounts die bekend zijn bij de telefoon op te halen. Dit kan alle accounts bevatten die zijn gemaakt door applicaties die je hebt geïnstalleerd.</string>
    <string name="permission_access_background_location_label">Toegang tot achtergrondlocatie</string>
    <string name="permission_access_background_location_description">Staat apps toe om op elk moment toegang te krijgen tot de locatie, zelfs wanneer ze niet in gebruik zijn.</string>
    <string name="permission_access_media_location_label">Toegang tot medialocatie</string>
    <string name="permission_access_media_location_description">Staat apps toe om locatie te lezen uit je mediacollectie.</string>
    <string name="permission_manage_accounts_label">Accounts beheren</string>
    <string name="permission_manage_accounts_description">Staat apps toe om accounts op het apparaat te beheren.</string>
    <string name="permission_wifi_control_label">WiFi-besturing</string>
    <string name="permission_wifi_control_description">Staat apps toe om WiFi aan of uit te zetten, WiFi-netwerken te scannen en ermee te verbinden, netwerken toe te voegen of te verwijderen, of een lokale hotspot te starten.</string>
    <string name="permission_change_network_state_label">Netwerkstatus wijzigen</string>
    <string name="permission_change_network_state_description">Staat apps toe om de status van netwerkconnectiviteit te wijzigen.</string>
    <string name="permission_read_sync_settings_label">Synchronisatie-instellingen lezen</string>
    <string name="permission_read_sync_settings_description">Staat apps toe om synchronisatie-instellingen voor een account te lezen.</string>
    <string name="permission_install_unknown_apps_label">Onbekende apps installeren (Onbekende bronnen)</string>
    <string name="permission_install_unknown_apps_description">Staat apps toe om andere apps op je apparaat te installeren (sideloaden). Dit kan je apparaat en gegevens in gevaar brengen.</string>
    <string name="permission_all_files_access_label">Toegang tot alle bestanden</string>
    <string name="permission_all_files_access_description">Staat apps toe om alle bestanden op je apparaat en alle aangesloten opslagapparaten te lezen, wijzigen en verwijderen. Apps met deze machtiging kunnen ook toegang krijgen tot bestanden die niet gerelateerd zijn aan de app, ZONDER je op de hoogte te stellen.</string>
    <string name="permission_appear_on_top_label">Bovenop verschijnen</string>
    <string name="permission_appear_on_top_description">Staat apps toe om dingen bovenop andere apps die je gebruikt te tonen (inclusief het tonen van advertenties). Dit kan interfereren met je gebruik van andere apps.</string>
    <string name="permission_files_and_media_label">Bestanden en media</string>
    <string name="permission_files_and_media_description">Staat apps toe om toegang te krijgen tot bestanden en media op je apparaat.</string>
    <string name="permission_access_to_media_only_label">Alleen toegang tot media</string>
    <string name="permission_access_to_media_only_description">Staat apps toe om alleen toegang te krijgen tot media op je apparaat.</string>
    <string name="permission_access_precise_location_label">Toegang tot nauwkeurige/precieze locatie</string>
    <string name="permission_access_precise_location_description">Staat apps toe om toegang te krijgen tot de nauwkeurige/precieze locatie van je apparaat. Dit wordt NIET aanbevolen voor de meeste apps (behalve voor sommige apps zoals kaarten, taxidiensten, apparaatvolgen-diensten zoals Zoek mijn apparaat enz.).</string>
    <string name="permission_access_approximate_location_label">Toegang tot geschatte locatie</string>
    <string name="permission_access_approximate_location_description">Staat apps toe om toegang te krijgen tot de geschatte locatie van je apparaat. Deze machtiging is voldoende voor de meeste apps die toegang tot je locatie nodig hebben voor kernfunctionaliteit.</string>
    <string name="permission_ignore_battery_optimizations_label">Batterijoptimalisaties negeren</string>
    <string name="permission_ignore_battery_optimizations_description">Staat apps toe om ongehinderd op de achtergrond te draaien. Dit kan de batterij leegtrekken. Alleen aanbevolen voor apps die continue/frequente back-ups en/of synchronisaties uitvoeren (bijv. Google Foto\'s), zeer frequent gebruikte apps (bijv. e-mail, chat, sms, enz.), evenals voor apps die een persistente melding hebben.</string>
    <string name="permission_notification_access_label">Toegang tot meldingen</string>
    <string name="permission_notification_access_description">Staat apps toe om al je meldingen te lezen, inclusief persoonlijke informatie (zoals contactnamen en tekst van berichten die je ontvangt), meldingen uit te stellen of weg te sturen of actieknoppen die ze bevatten te activeren (zoals telefoongesprekken beantwoorden, je sms/e-mail verwijderen, enz.) en Niet storen (DND) aan of uit te zetten en gerelateerde instellingen te wijzigen.</string>
    <string name="permission_do_not_disturb_permission_label">Niet storen (DND) machtiging</string>
    <string name="permission_do_not_disturb_permission_description">Staat apps toe om Niet storen (DND) aan of uit te zetten en gerelateerde instellingen te wijzigen.</string>
    <string name="permission_alarms_and_reminders_label">Alarmen en herinneringen</string>
    <string name="permission_alarms_and_reminders_description">Staat apps toe om alarmen in te stellen en tijdgevoelige acties te plannen. Dit stelt apps in staat om op de achtergrond te draaien, wat meer batterij kan gebruiken. Het uitschakelen van deze machtiging voor een app zou ervoor zorgen dat bestaande alarmen en tijdgebaseerde gebeurtenissen die door die specifieke app zijn gepland stoppen met werken.</string>
    <string name="permission_premium_sms_services_label">Premium SMS-toegang</string>
    <string name="permission_premium_sms_services_description">Staat apps toe om premium tekstberichtdiensten te gebruiken. Dit kost je geld.</string>
    <string name="permission_unlimited_data_access_label">Onbeperkte datatoegang</string>
    <string name="permission_unlimited_data_access_description">Staat apps toe om toegang te krijgen tot onbeperkte data. Apps kunnen data verbruiken zelfs wanneer Databesparing aan staat.</string>
    <string name="permission_group_calendar_label">Agenda</string>
    <string name="permission_group_calendar_description"><![CDATA[Agenda-gerelateerde machtigingen, bijv. items bekijken en bewerken.]]></string>
