    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Allows apps to communicate with Near Field Communication (NFC) tags, cards and readers.</string>
    <string name="permission_write_media_storage_label">Manage Media</string>
    <string name="permission_write_media_storage_description">Allows apps to modify or delete media files created with other apps WITHOUT asking you. Apps must have permission to access files and media.</string>
    <string name="permission_package_usage_stats_label">Usage Data Access</string>
    <string name="permission_package_usage_stats_description">Allows apps to track what other apps you use and how often, identify your service provider, language settings and other usage data.</string>
    <string name="permission_write_settings_label">Modify System Settings</string>
    <string name="permission_write_settings_description">Allows apps to modify system\'s settings data. Malicious apps may corrupt your system\'s configuration.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth Advertise</string>
    <string name="permission_bluetooth_advertise_description">Allows apps to advertise to nearby Bluetooth devices.</string>
    <string name="permission_get_accounts_label">Get Accounts</string>
    <string name="permission_get_accounts_description">Allows apps to get the list of accounts known by the phone. This may include any accounts created by applications that you have installed.</string>
    <string name="permission_access_background_location_label">Access Background Location</string>
    <string name="permission_access_background_location_description">Allows apps to access location any time, even while the app is not in use.</string>
    <string name="permission_access_media_location_label">Access Media Location</string>
    <string name="permission_access_media_location_description">Allows apps to read location from your media collection.</string>
    <string name="permission_manage_accounts_label">Manage Accounts</string>
    <string name="permission_manage_accounts_description">Allows apps to manage accounts in the device.</string>
    <string name="permission_change_wifi_state_label">Change WiFi state</string>
    <string name="permission_change_wifi_state_description">Allows apps to turn WiFi ON or OFF, scan and connect to WiFi networks, add or remove networks, or start a local-only HotSpot.</string>
    <string name="permission_change_network_state_label">Change Network state</string>
    <string name="permission_change_network_state_description">Allows apps to change the state of network connectivity.</string>
    <string name="permission_read_sync_settings_label">Read Sync Settings</string>
    <string name="permission_read_sync_settings_description">Allows apps to read sync settings for an account.</string>
    <string name="permission_request_install_packages_label">Install unknown apps (Unknown Sources)</string>
    <string name="permission_request_install_packages_description">Allows apps to install other apps on your device. This may put your device and data at risk.</string>
    <string name="permission_manage_external_storage_label">All Files access</string>
    <string name="permission_manage_external_storage_description">Allows apps to read, modify and delete all files on your device as well as any connected storage devices. Apps with this permission may also access files that aren\'t related to the app, WITHOUT notifying you.</string>
    <string name="permission_system_alert_window_label">Appear on Top</string>
    <string name="permission_system_alert_window_description">Allows apps to show things on top of other apps you\'re using (including showing ads). This may interfere with your use of other apps.</string>
    <string name="permission_group_calendar_label">Calendar</string>
    <string name="permission_group_calendar_description"><![CDATA[Calendar related permissions, e.g. view & edit entries.]]></string>
    <string name="permission_group_files_label">Files and media</string>
    <string name="permission_group_files_description">File related permissions. Access to storage, documents and media.</string>
