    <string name="permission_write_calendar_label">Písať do kalendára</string>
    <string name="permission_write_calendar_description">Umožňuje pridávanie, odstraňovanie a zmenu udalostí kalendára vo vašom zariadení.</string>
    <string name="permission_read_calendar_label">Čítať kalendár</string>
    <string name="permission_read_calendar_description">Umožňuje čítanie udalostí kalendára uložených vo vašom zariadení.</string>
    <string name="permission_write_contacts_label">Upravovať kontakty</string>
    <string name="permission_write_contacts_description">Umožňuje úpravu kontaktných údajov uložených vo vašom zariadení.</string>
    <string name="permission_read_contacts_label">Čítať kontakty</string>
    <string name="permission_read_contacts_description">Umožňuje čítanie kontaktných údajov uložených vo vašom zariadení.</string>
    <string name="permission_get_accounts_label">Nájsť účty</string>
    <string name="permission_get_accounts_description">Umožňuje aplikáciám získať zoznam účtov známych telefónu. Môže to zahŕňať účty vytvorené nainštalovanými aplikáciami.</string>
    <string name="permission_access_background_location_label">Prístup k polohe na pozadí</string>
    <string name="permission_access_background_location_description">Umožňuje aplikáciám pristupovať k polohe kedykoľvek, aj keď sa nepoužívajú.</string>
    <string name="permission_access_media_location_label">Prístup k polohe médií</string>
    <string name="permission_access_media_location_description">Umožňuje aplikáciám čítať informácie o polohe z vašej kolekcie médií.</string>
    <string name="permission_manage_accounts_label">Spravovať účty</string>
    <string name="permission_manage_accounts_description">Umožňuje aplikáciám spravovať účty v zariadení.</string>
    <string name="permission_wifi_control_label">Ovládanie WiFi</string>
    <string name="permission_wifi_control_description">Umožňuje aplikáciám zapínať alebo vypínať WiFi, skenovať a pripájať sa k WiFi sieťam, pridávať alebo odstraňovať siete alebo spúšťať lokálny hotspot.</string>
    <string name="permission_change_network_state_label">Zmeniť stav siete</string>
    <string name="permission_change_network_state_description">Umožňuje aplikáciám meniť stav pripojenia k sieti.</string>
    <string name="permission_read_sync_settings_label">Čítať nastavenia synchronizácie</string>
    <string name="permission_read_sync_settings_description">Umožňuje aplikáciám čítať nastavenia synchronizácie pre účet.</string>
    <string name="permission_install_unknown_apps_label">Inštalovať neznáme aplikácie (Neznáme zdroje)</string>
    <string name="permission_install_unknown_apps_description">Umožňuje aplikáciám inštalovať (sideload) iné aplikácie do zariadenia. Môže to ohroziť zariadenie a údaje.</string>
    <string name="permission_all_files_access_label">Prístup ku všetkým súborom</string>
    <string name="permission_all_files_access_description">Umožňuje aplikáciám čítať, upravovať a mazať všetky súbory zo zariadenia a z akéhokoľvek pripojeného úložiska. Aplikácie s týmto oprávnením môžu pristupovať k súborom BEZ oznámenia.</string>
    <string name="permission_appear_on_top_label">Zobraziť navrchu</string>
    <string name="permission_appear_on_top_description">Umožňuje aplikáciám zobrazovať veci nad inými používanými aplikáciami (vrátane zobrazenia reklám). Môže to narúšať používanie iných aplikácií.</string>
    <string name="permission_files_and_media_label">Súbory a médiá</string>
    <string name="permission_files_and_media_description">Umožňuje aplikáciám pristupovať k súborom a médiám v zariadení.</string>
    <string name="permission_access_to_media_only_label">Prístup len k médiám</string>
    <string name="permission_access_to_media_only_description">Umožňuje aplikáciám pristupovať len k médiám v zariadení.</string>
    <string name="permission_access_precise_location_label">Prístup k presnej polohe</string>
    <string name="permission_access_precise_location_description">Umožňuje aplikáciám používať presnú polohu zariadenia. Toto sa NEodporúča pre väčšinu aplikácií (okrem niektorých ako mapy, taxi služby, sledovacie služby ako Find My Device atď.).</string>
    <string name="permission_access_approximate_location_label">Prístup k približnej polohe</string>
    <string name="permission_access_approximate_location_description">Umožňuje aplikáciám používať približnú polohu zariadenia. Toto oprávnenie je dostatočné pre väčšinu aplikácií, ktoré potrebujú polohu pre základné funkcie.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorovať optimalizácie batérie</string>
    <string name="permission_ignore_battery_optimizations_description">Umožňuje aplikáciám bežať neobmedzene na pozadí. Môže to vyčerpávať batériu. Odporúča sa len pre aplikácie, ktoré robia nepretržité/časté zálohy a synchronizácie (napr. Google Photos), veľmi často používané aplikácie (napr. email, chat, SMS atď.) a aplikácie s trvalými oznámeniami.</string>
    <string name="permission_notification_access_label">Prístup k oznámeniam</string>
    <string name="permission_notification_access_description">Umožňuje aplikáciám čítať všetky oznámenia vrátane osobných informácií (ako mená kontaktov a text prijatých správ), odkladať alebo zamietať oznámenia, alebo spúšťať tlačidlá akcií (ako odpovedanie na hovory, mazanie SMS/emailov atď.) a zapínať/vypínať režim Nerušiť (DND) a meniť súvisiace nastavenia.</string>
    <string name="permission_do_not_disturb_permission_label">Oprávnenie Nerušiť (DND)</string>
    <string name="permission_do_not_disturb_permission_description">Umožňuje aplikáciám zapínať/vypínať režim Nerušiť (DND) a meniť súvisiace nastavenia.</string>
    <string name="permission_alarms_and_reminders_label">Budíky a pripomienky</string>
    <string name="permission_alarms_and_reminders_description">Umožňuje aplikáciám nastavovať budíky a plánovať časovo citlivé akcie. Umožňuje aplikáciám bežať na pozadí, čo môže spotrebovať viac batérie. Vypnutie tohto oprávnenia pre aplikáciu spôsobí, že naplánované budíky a časovo založené udalosti prestanú fungovať.</string>
    <string name="permission_premium_sms_services_label">Prístup k prémilovým SMS službám</string>
    <string name="permission_premium_sms_services_description">Umožňuje aplikáciám používať prémiové textové služby. Bude vás to stáť peniaze.</string>
    <string name="permission_unlimited_data_access_label">Neobmedzený prístup k údajom</string>
    <string name="permission_unlimited_data_access_description">Umožňuje aplikáciám pristupovať k neobmedzeným údajom. Aplikácie môžu spotrebovávať údaje aj keď je Šetrič údajov ZAPNUTÝ.</string>
    <string name="permission_group_calendar_label">Kalendár</string>
    <string name="permission_group_calendar_description"><![CDATA[Oprávnenia súvisiace s kalendárom, napr. zobrazovanie a úprava záznamov.]]></string>
    <string name="permission_group_files_label">Súbory a médiá</string>
    <string name="permission_group_files_description">Oprávnenia súvisiace so súbormi. Prístup k úložisku, dokumentom a médiám.</string>
    <string name="permission_group_other_label">Ostatné</string>
    <string name="permission_group_other_description">Ostatné oprávnenia bez špecifickej kategórie.</string>
    <string name="permission_group_messaging_label">Správy</string>
    <string name="permission_group_messaging_description">SMS, MMS a ďalšie. Všetko súvisiace so správami.</string>
    <string name="permission_group_connectivity_label">Pripojenie</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth a ďalšie. Oprávnenia súvisiace s komunikáciou so svetom.</string>
    <string name="permission_group_sensors_label">Senzory</string>
    <string name="permission_group_sensors_description">Akcelerometer, gyroskop, telesné senzory, rozpoznávanie aktivity a ďalšie. Spôsoby, ako môže vaše zariadenie zbierať vstup.</string>
    <string name="permission_group_calls_label">Hovory</string>
