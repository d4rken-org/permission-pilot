    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/exampleFragment">
    <fragment
        android:id="@+id/exampleFragment"
        android:name="eu.darken.myperm.main.ui.main.MainFragment"
        tools:layout="@layout/main_fragment"
        android:label="@string/label_example_fragment">
        <argument
            android:name="exampleArgument"
            app:argType="string" />
        <action
            android:id="@+id/action_exampleFragment_to_settingsContainerFragment"
            app:destination="@id/settingsContainerFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsContainerFragment"
        android:name="eu.darken.myperm.settings.ui.SettingsFragment"
        android:label="SettingsContainerFragment" />
