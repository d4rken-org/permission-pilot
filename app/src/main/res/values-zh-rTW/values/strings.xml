    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Allows apps to communicate with Near Field Communication (NFC) tags, cards and readers.</string>
    <string name="permission_manage_media_label">Manage Media</string>
    <string name="permission_manage_media_description">Allows apps to modify or delete media files created with other apps WITHOUT asking you. Apps must have permission to access files and media.</string>
    <string name="permission_usage_data_access_label">Usage Data Access</string>
    <string name="permission_usage_data_access_description">Allows apps to track what other apps you use and how often, identify your service provider, language settings and other usage data.</string>
    <string name="permission_modify_system_settings_label">Modify System Settings</string>
    <string name="permission_modify_system_settings_description">Allows apps to modify system\'s settings data. Malicious apps may corrupt your system\'s configuration.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth Advertise</string>
    <string name="permission_bluetooth_advertise_description">Allows apps to advertise to nearby Bluetooth devices.</string>
    <string name="permission_get_accounts_label">Get Accounts</string>
    <string name="permission_get_accounts_description">Allows apps to get the list of accounts known by the phone. This may include any accounts created by applications that you have installed.</string>
    <string name="permission_access_background_location_label">Access Background Location</string>
    <string name="permission_access_background_location_description">Allows apps to access location any time, even when they are not in use.</string>
    <string name="permission_access_media_location_label">Access Media Location</string>
    <string name="permission_access_media_location_description">Allows apps to read location from your media collection.</string>
    <string name="permission_manage_accounts_label">Manage Accounts</string>
    <string name="permission_manage_accounts_description">Allows apps to manage accounts in the device.</string>
    <string name="permission_wifi_control_label">WiFi Control</string>
    <string name="permission_wifi_control_description">Allows apps to turn WiFi ON or OFF, scan and connect to WiFi networks, add or remove networks, or start a local-only HotSpot.</string>
    <string name="permission_change_network_state_label">Change Network state</string>
    <string name="permission_change_network_state_description">Allows apps to change the state of network connectivity.</string>
    <string name="permission_read_sync_settings_label">Read Sync Settings</string>
    <string name="permission_read_sync_settings_description">Allows apps to read sync settings for an account.</string>
    <string name="permission_install_unknown_apps_label">Install unknown apps (Unknown Sources)</string>
    <string name="permission_install_unknown_apps_description">Allows apps to install (sideload) other apps on your device. This may put your device and data at risk.</string>
    <string name="permission_all_files_access_label">All Files access</string>
    <string name="permission_all_files_access_description">Allows apps to read, modify and delete all files on your device as well as any connected storage devices. Apps with this permission may also access files that aren\'t related to the app, WITHOUT notifying you.</string>
    <string name="permission_appear_on_top_label">Appear on Top</string>
    <string name="permission_appear_on_top_description">Allows apps to show things on top of other apps you\'re using (including showing ads). This may interfere with your use of other apps.</string>
    <string name="permission_files_and_media_label">Files and Media</string>
    <string name="permission_files_and_media_description">Allows apps access to files and media on your device.</string>
    <string name="permission_access_to_media_only_label">Access to Media only</string>
    <string name="permission_access_to_media_only_description">Allows apps access to media only on your device.</string>
    <string name="permission_access_precise_location_label">Access accurate/ precise location</string>
    <string name="permission_access_precise_location_description">Allows apps to access your device\'s accurate/ precise location. This is NOT recommended for most apps (except for some apps like maps, cab services, device tracking services like Find My Device etc.).</string>
    <string name="permission_access_approximate_location_label">Access approximate location</string>
    <string name="permission_access_approximate_location_description">Allows apps to access your device\'s approximate location. This permission is sufficient for most apps that need access to your location for core functionality.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignore Battery Optimizations</string>
    <string name="permission_ignore_battery_optimizations_description">Allows apps to run unhindered in the background. This may drain battery. Recommended only for apps that perform continuous/ frequent backups and/ or syncs (e.g. Google Photos), very frequently used apps (e.g. email, chat, sms, etc.), as well as for those apps that have a persistent notification.</string>
    <string name="permission_notification_access_label">Notification Access</string>
    <string name="permission_notification_access_description">Allows apps to read all your notifications, including personal information (such as contact names and text of messages you receive), snooze or dismiss notifications or trigger action buttons they contain (such as answer phone calls, delete your sms/ email, etc) and turn Do Not Disturb (DND) ON or OFF and change related settings.</string>
    <string name="permission_do_not_disturb_permission_label">Do Not Disturb (DND) Permission</string>
    <string name="permission_do_not_disturb_permission_description">Allows apps to turn Do Not Disturb (DND) ON or OFF and change related settings.</string>
    <string name="permission_alarms_and_reminders_label">Alarms and Reminders</string>
    <string name="permission_alarms_and_reminders_description">Allows apps to set alarms and schedule time-sensitive actions. This allows apps to run in the background, which may use more battery. Turning OFF this permission for an app would cause existing alarms and time-based events that are scheduled by that particular app to stop working.</string>
    <string name="permission_premium_sms_services_label">Premium SMS Access</string>
    <string name="permission_premium_sms_services_description">Allows apps to use premium text message services. This will cost you money.</string>
    <string name="permission_unlimited_data_access_label">Unlimited Data Access</string>
    <string name="permission_unlimited_data_access_description">Allows apps access to unrestricted data. Apps can consume data even when Data Saver is ON.</string>
    <string name="permission_group_calendar_label">Calendar</string>
    <string name="permission_group_calendar_description"><![CDATA[Calendar related permissions, e.g. view & edit entries.]]></string>
    <string name="permission_group_files_label">Files and Media</string>
    <string name="permission_group_files_description">File related permissions. Access to storage, documents and media.</string>
    <string name="permission_group_other_label">Other</string>
    <string name="permission_group_other_description">Other permissions without specific category.</string>
    <string name="permission_group_messaging_label">Messaging</string>
    <string name="permission_group_messaging_description">SMS, MMS and more. Everything messaging related.</string>
    <string name="permission_group_connectivity_label">Connectivity</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth and more. Permissions related to communicating with the outside world.</string>
    <string name="permission_group_sensors_label">Sensors</string>
    <string name="permission_group_sensors_description">Accelerometer, Gyro, Body Sensors, Activity Recognition, and more. Ways that your device can capture input.</string>
