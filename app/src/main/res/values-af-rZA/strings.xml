    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Laat apps toe om te kommunikeer met Near Field Communication (NFC) etikette, kaarte en lesers.</string>
    <string name="permission_manage_media_label">Bestuur Media</string>
    <string name="permission_manage_media_description">Laat apps toe om media lêers wat met ander apps geskep is te wysig of uit te vee SONDER om jou te vra. Apps moet toestemming hê om toegang tot lêers en media te kry.</string>
    <string name="permission_usage_data_access_label">Gebruik Data Toegang</string>
    <string name="permission_usage_data_access_description">Laat apps toe om te volg watter ander apps jy gebruik en hoe gereeld, identifiseer jou diens verskaffer, taal instellings en ander gebruik data.</string>
    <string name="permission_modify_system_settings_label">Wysig Stelsel Instellings</string>
    <string name="permission_modify_system_settings_description">Laat apps toe om die stelsel se instellings data te wysig. Kwaadwillige apps kan jou stelsel se konfigurasie korrupteer.</string>
    <string name="permission_bluetooth_advertise_label">Bluetooth Adverteer</string>
    <string name="permission_bluetooth_advertise_description">Laat apps toe om te adverteer na naby Bluetooth toestelle.</string>
    <string name="permission_get_accounts_label">Kry Rekeninge</string>
    <string name="permission_get_accounts_description">Laat apps toe om die lys van rekeninge wat bekend is deur die foon te kry. Dit kan enige rekeninge insluit wat geskep is deur toepassings wat jy geïnstalleer het.</string>
    <string name="permission_access_background_location_label">Toegang Agtergrond Ligging</string>
    <string name="permission_access_background_location_description">Laat apps toe om ligging enige tyd te benader, selfs wanneer hulle nie in gebruik is nie.</string>
    <string name="permission_access_media_location_label">Toegang Media Ligging</string>
    <string name="permission_access_media_location_description">Laat apps toe om ligging van jou media versameling te lees.</string>
    <string name="permission_manage_accounts_label">Bestuur Rekeninge</string>
    <string name="permission_manage_accounts_description">Laat apps toe om rekeninge in die toestel te bestuur.</string>
    <string name="permission_wifi_control_label">WiFi Beheer</string>
    <string name="permission_wifi_control_description">Laat apps toe om WiFi AAN of AF te skakel, skandeer en koppel aan WiFi netwerke, voeg by of verwyder netwerke, of begin \'n plaaslike-alleen HotSpot.</string>
    <string name="permission_change_network_state_label">Verander Netwerk toestand</string>
    <string name="permission_change_network_state_description">Laat apps toe om die toestand van netwerk konnektiwiteit te verander.</string>
    <string name="permission_read_sync_settings_label">Lees Sinkroniseer Instellings</string>
    <string name="permission_read_sync_settings_description">Laat apps toe om sinkroniseer instellings vir \'n rekening te lees.</string>
    <string name="permission_install_unknown_apps_label">Installeer onbekende apps (Onbekende Bronne)</string>
    <string name="permission_install_unknown_apps_description">Laat apps toe om ander apps op jou toestel te installeer (sylaai). Dit kan jou toestel en data in gevaar stel.</string>
    <string name="permission_all_files_access_label">Alle Lêers toegang</string>
    <string name="permission_all_files_access_description">Laat apps toe om alle lêers op jou toestel asook enige gekoppelde stoorplek toestelle te lees, wysig en uit te vee. Apps met hierdie toestemming kan ook toegang kry tot lêers wat nie verwant is aan die app nie, SONDER om jou in kennis te stel.</string>
    <string name="permission_appear_on_top_label">Verskyn Bo-op</string>
    <string name="permission_appear_on_top_description">Laat apps toe om goed bo-op ander apps wat jy gebruik te wys (insluitend advertensies). Dit kan inmeng met jou gebruik van ander apps.</string>
    <string name="permission_files_and_media_label">Lêers en Media</string>
    <string name="permission_files_and_media_description">Laat apps toegang tot lêers en media op jou toestel toe.</string>
    <string name="permission_access_to_media_only_label">Toegang tot Media slegs</string>
    <string name="permission_access_to_media_only_description">Laat apps toegang tot slegs media op jou toestel toe.</string>
    <string name="permission_access_precise_location_label">Toegang akkurate/presiese ligging</string>
    <string name="permission_access_precise_location_description">Laat apps toe om jou toestel se akkurate/presiese ligging te benader. Dit word NIE aanbeveel vir meeste apps nie (behalwe vir sommige apps soos kaarte, taxi dienste, toestel opsporing dienste soos Find My Device ens.).</string>
    <string name="permission_access_approximate_location_label">Toegang benaderde ligging</string>
    <string name="permission_access_approximate_location_description">Laat apps toe om jou toestel se benaderde ligging te benader. Hierdie toestemming is voldoende vir meeste apps wat toegang tot jou ligging nodig het vir kern funksionaliteit.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignoreer Battery Optimaliserings</string>
    <string name="permission_ignore_battery_optimizations_description">Laat apps toe om ongehinderd in die agtergrond te loop. Dit kan battery drain. Aanbeveel slegs vir apps wat deurlopende/gereelde rugsteun en/of sinkronisasies uitvoer (bv. Google Photos), baie gereeld gebruikte apps (bv. epos, gesels, sms, ens.), asook vir daardie apps wat \'n volhoudende kennisgewing het.</string>
    <string name="permission_notification_access_label">Kennisgewing Toegang</string>
    <string name="permission_notification_access_description">Laat apps toe om al jou kennisgewings te lees, insluitend persoonlike inligting (soos kontak name en teks van boodskappe wat jy ontvang), sluimer of ontslaan kennisgewings of trigger aksie knoppies wat hulle bevat (soos antwoord telefoon oproepe, vee jou sms/epos uit, ens.) en skakel Moenie Steur (DND) AAN of AF en verander verwante instellings.</string>
    <string name="permission_do_not_disturb_permission_label">Moenie Steur (DND) Toestemming</string>
    <string name="permission_do_not_disturb_permission_description">Laat apps toe om Moenie Steur (DND) AAN of AF te skakel en verwante instellings te verander.</string>
    <string name="permission_alarms_and_reminders_label">Alarms en Herinneringe</string>
    <string name="permission_alarms_and_reminders_description">Laat apps toe om alarms op te stel en tyd-sensitiewe aksies te skeduleer. Dit laat apps toe om in die agtergrond te loop, wat meer battery kan gebruik. Om hierdie toestemming AF te skakel vir \'n app sal bestaande alarms en tyd-gebaseerde gebeurtenisse wat geskeduleer is deur daardie spesifieke app laat ophou werk.</string>
    <string name="permission_premium_sms_services_label">Premium SMS Toegang</string>
    <string name="permission_premium_sms_services_description">Laat apps toe om premium teks boodskap dienste te gebruik. Dit sal jou geld kos.</string>
    <string name="permission_unlimited_data_access_label">Onbeperkte Data Toegang</string>
    <string name="permission_unlimited_data_access_description">Laat apps toegang tot onbeperkte data toe. Apps kan data verbruik selfs wanneer Data Bespaarder AAN is.</string>
    <string name="permission_group_calendar_label">Kalender</string>
    <string name="permission_group_calendar_description"><![CDATA[Kalender verwante toestemmings, bv. bekyk & redigeer inskrywings.]]></string>
