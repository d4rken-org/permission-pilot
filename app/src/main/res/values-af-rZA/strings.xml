    <string name="permission_group_sensors_label">Sensors</string>
    <string name="permission_group_sensors_description">Versnellingsmeter, Giro, Liggaam Sensors, Aktiwiteit Erkenning, en meer. Maniere dat jou toestel invoer kan vang.</string>
