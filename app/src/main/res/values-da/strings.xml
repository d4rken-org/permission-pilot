    <string name="permission_write_calendar_label">Skriv kalender</string>
    <string name="permission_write_calendar_description">Tillader at tilføje, fjerne og ændre kalenderbegivenheder på din enhed.</string>
    <string name="permission_read_calendar_label">Læs kalender</string>
    <string name="permission_read_calendar_description">Tillader at læse kalenderbegivenheder gemt på din enhed.</string>
    <string name="permission_write_contacts_label">Ændr kontakter</string>
    <string name="permission_write_contacts_description">Tillader at ændre kontaktdata gemt på din enhed.</string>
    <string name="permission_read_contacts_label">Læs kontakter</string>
    <string name="permission_read_contacts_description">Tillader at læse kontaktdata gemt på din enhed.</string>
    <string name="permission_get_accounts_label">Find konti</string>
    <string name="permission_get_accounts_description">Tillader apps at få listen over konti kendt af telefonen. Dette kan inkludere konti oprettet af applikationer, du har installeret.</string>
    <string name="permission_access_background_location_label">Adgang til baggrundslokation</string>
    <string name="permission_access_background_location_description">Tillader apps at få adgang til lokation når som helst, selv når de ikke er i brug.</string>
    <string name="permission_access_media_location_label">Adgang til medielokation</string>
    <string name="permission_access_media_location_description">Tillader apps at læse lokation fra din mediesamling.</string>
    <string name="permission_manage_accounts_label">Administrer konti</string>
    <string name="permission_manage_accounts_description">Tillader apps at administrere konti på enheden.</string>
    <string name="permission_wifi_control_label">WiFi-kontrol</string>
    <string name="permission_wifi_control_description">Tillader apps at tænde eller slukke for WiFi, scanne og forbinde til WiFi-netværk, tilføje eller fjerne netværk, eller starte et lokalt hotspot.</string>
    <string name="permission_change_network_state_label">Ændr netværkstilstand</string>
    <string name="permission_change_network_state_description">Tillader apps at ændre tilstanden af netværksforbindelse.</string>
    <string name="permission_read_sync_settings_label">Læs synkroniseringsindstillinger</string>
    <string name="permission_read_sync_settings_description">Tillader apps at læse synkroniseringsindstillinger for en konto.</string>
    <string name="permission_install_unknown_apps_label">Installér ukendte apps (Ukendte kilder)</string>
    <string name="permission_install_unknown_apps_description">Tillader apps at installere (sideloade) andre apps på din enhed. Dette kan sætte din enhed og data i fare.</string>
    <string name="permission_all_files_access_label">Alle filers adgang</string>
    <string name="permission_all_files_access_description">Tillader apps at læse, ændre og slette alle filer på din enhed samt alle tilsluttede lagerenheder. Apps med denne tilladelse kan også få adgang til filer, der ikke er relateret til appen, UDEN at give dig besked.</string>
    <string name="permission_appear_on_top_label">Vis øverst</string>
    <string name="permission_appear_on_top_description">Tillader apps at vise ting oven på andre apps, du bruger (inklusive visning af reklamer). Dette kan forstyrre din brug af andre apps.</string>
    <string name="permission_files_and_media_label">Filer og medier</string>
    <string name="permission_files_and_media_description">Tillader apps adgang til filer og medier på din enhed.</string>
    <string name="permission_access_to_media_only_label">Kun adgang til medier</string>
    <string name="permission_access_to_media_only_description">Tillader apps kun adgang til medier på din enhed.</string>
    <string name="permission_access_precise_location_label">Adgang til præcis lokation</string>
    <string name="permission_access_precise_location_description">Tillader apps at få adgang til din enheds præcise lokation. Dette anbefales IKKE for de fleste apps (undtagen for nogle apps som kort, taxitjenester, enhedssporingstjenester som Find My Device osv.).</string>
    <string name="permission_access_approximate_location_label">Adgang til omtrentlig lokation</string>
    <string name="permission_access_approximate_location_description">Tillader apps at få adgang til din enheds omtrentlige lokation. Denne tilladelse er tilstrækkelig for de fleste apps, der har brug for adgang til din lokation for kernefunktionalitet.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorer batterioptimeringer</string>
    <string name="permission_ignore_battery_optimizations_description">Tillader apps at køre uhindret i baggrunden. Dette kan dræne batteriet. Anbefales kun til apps, der udfører kontinuerlige/hyppige sikkerhedskopier og/eller synkroniseringer (f.eks. Google Photos), meget hyppigt brugte apps (f.eks. e-mail, chat, sms osv.), samt for apps med en vedvarende notifikation.</string>
    <string name="permission_notification_access_label">Notifikationsadgang</string>
    <string name="permission_notification_access_description">Tillader apps at læse alle dine notifikationer, inklusive personlige oplysninger (som kontaktnavne og tekst i beskeder, du modtager), udsætte eller afvise notifikationer eller udløse handlingsknapper de indeholder (som at besvare telefonopkald, slette dine sms/e-mails osv.) og tænde Forstyr ikke (DND) TIL eller FRA og ændre relaterede indstillinger.</string>
    <string name="permission_do_not_disturb_permission_label">Forstyr ikke (DND) tilladelse</string>
    <string name="permission_do_not_disturb_permission_description">Tillader apps at tænde Forstyr ikke (DND) TIL eller FRA og ændre relaterede indstillinger.</string>
    <string name="permission_alarms_and_reminders_label">Alarmer og påmindelser</string>
    <string name="permission_alarms_and_reminders_description">Tillader apps at indstille alarmer og planlægge tidsfølsomme handlinger. Dette tillader apps at køre i baggrunden, hvilket kan bruge mere batteri. At slå denne tilladelse FRA for en app vil forårsage, at eksisterende alarmer og tidsbaserede begivenheder, der er planlagt af den pågældende app, holder op med at virke.</string>
    <string name="permission_premium_sms_services_label">Premium SMS-adgang</string>
    <string name="permission_premium_sms_services_description">Tillader apps at bruge premium tekstbeskedtjenester. Dette vil koste dig penge.</string>
    <string name="permission_unlimited_data_access_label">Ubegrænset dataadgang</string>
    <string name="permission_unlimited_data_access_description">Tillader apps adgang til ubegrænsede data. Apps kan forbruge data, selv når Dataspar er TIL.</string>
