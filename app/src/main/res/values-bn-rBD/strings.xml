    <string name="overview_device_android_version_label">Android সংস্করণ</string>
    <string name="overview_device_name_label">ডিভাইস</string>
    <string name="overview_device_android_patch_label">প্যাচ স্তর</string>
    <string name="overview_summary_apps_active_profile_label">সক্রিয় প্রোফাইলে অ্যাপস</string>
    <string name="overview_summary_apps_other_profile_label">পরিচালিত ব্যবহারকারী প্রোফাইলে অ্যাপস, ক্লোন বাদে</string>
    <string name="overview_summary_apps_sideloaded_label">বাহ্যিক উৎস থেকে সাইডলোড করা অ্যাপস</string>
    <string name="overview_summary_apps_installers_label">\'অজানা উৎস থেকে ইনস্টল\' সক্রিয় অ্যাপস</string>
    <string name="overview_summary_apps_overlayers_label">যে অ্যাপগুলি \'উপরে দেখান\' করতে পারে</string>
