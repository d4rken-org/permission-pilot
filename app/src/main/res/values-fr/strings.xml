    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Permet aux applications de communiquer avec les étiquettes, cartes et lecteurs Near Field Communication (NFC).</string>
    <string name="permission_manage_media_label">Gérer les médias</string>
    <string name="permission_manage_media_description">Permet aux applications de modifier ou supprimer des fichiers multimédias créés avec d\'autres applications SANS vous demander. Les applications doivent avoir l\'autorisation d\'accéder aux fichiers et aux médias.</string>
    <string name="permission_usage_data_access_label">Accès aux données d\'utilisation</string>
    <string name="permission_usage_data_access_description">Permet aux applications de suivre quelles autres applications vous utilisez et à quelle fréquence, d\'identifier votre fournisseur de services, les paramètres de langue et autres données d\'utilisation.</string>
    <string name="permission_modify_system_settings_label">Modifier les paramètres système</string>
    <string name="permission_modify_system_settings_description">Permet aux applications de modifier les données des paramètres système. Les applications malveillantes peuvent corrompre la configuration de votre système.</string>
    <string name="permission_bluetooth_advertise_label">Publicité Bluetooth</string>
    <string name="permission_bluetooth_advertise_description">Permet aux applications de faire de la publicité vers les appareils Bluetooth à proximité.</string>
    <string name="permission_get_accounts_label">Obtenir les comptes</string>
    <string name="permission_get_accounts_description">Permet aux applications d\'obtenir la liste des comptes connus par le téléphone. Cela peut inclure tous les comptes créés par les applications que vous avez installées.</string>
    <string name="permission_access_background_location_label">Accès à la localisation en arrière-plan</string>
    <string name="permission_access_background_location_description">Permet aux applications d\'accéder à la localisation à tout moment, même lorsqu\'elles ne sont pas utilisées.</string>
    <string name="permission_access_media_location_label">Accès à la localisation des médias</string>
    <string name="permission_access_media_location_description">Permet aux applications de lire la localisation de votre collection de médias.</string>
    <string name="permission_manage_accounts_label">Gérer les comptes</string>
    <string name="permission_manage_accounts_description">Permet aux applications de gérer les comptes de l\'appareil.</string>
    <string name="permission_wifi_control_label">Contrôle WiFi</string>
    <string name="permission_wifi_control_description">Permet aux applications d\'activer ou désactiver le WiFi, de scanner et se connecter aux réseaux WiFi, d\'ajouter ou supprimer des réseaux, ou de démarrer un point d\'accès local uniquement.</string>
    <string name="permission_change_network_state_label">Changer l\'état du réseau</string>
    <string name="permission_change_network_state_description">Permet aux applications de changer l\'état de la connectivité réseau.</string>
    <string name="permission_read_sync_settings_label">Lire les paramètres de synchronisation</string>
    <string name="permission_read_sync_settings_description">Permet aux applications de lire les paramètres de synchronisation d\'un compte.</string>
    <string name="permission_install_unknown_apps_label">Installer des applications inconnues (Sources inconnues)</string>
    <string name="permission_install_unknown_apps_description">Permet aux applications d\'installer (charger latéralement) d\'autres applications sur votre appareil. Cela peut mettre votre appareil et vos données en danger.</string>
    <string name="permission_all_files_access_label">Accès à tous les fichiers</string>
    <string name="permission_all_files_access_description">Permet aux applications de lire, modifier et supprimer tous les fichiers sur votre appareil ainsi que tous les périphériques de stockage connectés. Les applications avec cette autorisation peuvent également accéder aux fichiers qui ne sont pas liés à l\'application, SANS vous en informer.</string>
    <string name="permission_appear_on_top_label">Apparaître au-dessus</string>
    <string name="permission_appear_on_top_description">Permet aux applications d\'afficher des éléments au-dessus d\'autres applications que vous utilisez (y compris afficher des publicités). Cela peut interférer avec votre utilisation d\'autres applications.</string>
    <string name="permission_files_and_media_label">Fichiers et médias</string>
    <string name="permission_files_and_media_description">Permet aux applications d\'accéder aux fichiers et médias sur votre appareil.</string>
    <string name="permission_access_to_media_only_label">Accès aux médias uniquement</string>
    <string name="permission_access_to_media_only_description">Permet aux applications d\'accéder uniquement aux médias sur votre appareil.</string>
    <string name="permission_access_precise_location_label">Accès à la localisation précise/exacte</string>
    <string name="permission_access_precise_location_description">Permet aux applications d\'accéder à la localisation précise/exacte de votre appareil. Ceci n\'est PAS recommandé pour la plupart des applications (sauf pour certaines applications comme les cartes, les services de taxi, les services de suivi d\'appareil comme Localiser mon appareil, etc.).</string>
    <string name="permission_access_approximate_location_label">Accès à la localisation approximative</string>
    <string name="permission_access_approximate_location_description">Permet aux applications d\'accéder à la localisation approximative de votre appareil. Cette autorisation est suffisante pour la plupart des applications qui ont besoin d\'accéder à votre localisation pour des fonctionnalités principales.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorer les optimisations de batterie</string>
    <string name="permission_ignore_battery_optimizations_description">Permet aux applications de fonctionner sans entrave en arrière-plan. Cela peut vider la batterie. Recommandé uniquement pour les applications qui effectuent des sauvegardes et/ou synchronisations continues/fréquentes (par exemple Google Photos), des applications très fréquemment utilisées (par exemple e-mail, chat, SMS, etc.), ainsi que pour les applications qui ont une notification persistante.</string>
    <string name="permission_notification_access_label">Accès aux notifications</string>
    <string name="permission_notification_access_description">Permet aux applications de lire toutes vos notifications, y compris les informations personnelles (comme les noms de contacts et le texte des messages que vous recevez), de reporter ou rejeter les notifications ou déclencher les boutons d\'action qu\'elles contiennent (comme répondre aux appels téléphoniques, supprimer vos SMS/e-mails, etc.) et d\'activer ou désactiver Ne pas déranger (NPD) et modifier les paramètres associés.</string>
    <string name="permission_do_not_disturb_permission_label">Autorisation Ne pas déranger (NPD)</string>
    <string name="permission_do_not_disturb_permission_description">Permet aux applications d\'activer ou désactiver Ne pas déranger (NPD) et de modifier les paramètres associés.</string>
    <string name="permission_alarms_and_reminders_label">Alarmes et rappels</string>
    <string name="permission_alarms_and_reminders_description">Permet aux applications de définir des alarmes et programmer des actions sensibles au temps. Cela permet aux applications de fonctionner en arrière-plan, ce qui peut utiliser plus de batterie. Désactiver cette autorisation pour une application ferait que les alarmes existantes et les événements basés sur le temps programmés par cette application particulière cessent de fonctionner.</string>
    <string name="permission_premium_sms_services_label">Accès SMS Premium</string>
    <string name="permission_premium_sms_services_description">Permet aux applications d\'utiliser des services de messages texte premium. Cela vous coûtera de l\'argent.</string>
    <string name="permission_unlimited_data_access_label">Accès illimité aux données</string>
    <string name="permission_unlimited_data_access_description">Permet aux applications d\'accéder à des données illimitées. Les applications peuvent consommer des données même lorsque l\'Économiseur de données est activé.</string>
    <string name="permission_group_calendar_label">Calendrier</string>
    <string name="permission_group_calendar_description"><![CDATA[Autorisations liées au calendrier, par exemple voir et modifier les entrées.]]></string>
    <string name="permission_group_files_label">Fichiers et médias</string>
    <string name="permission_group_files_description">Autorisations liées aux fichiers. Accès au stockage, aux documents et aux médias.</string>
    <string name="permission_group_other_label">Autres</string>
