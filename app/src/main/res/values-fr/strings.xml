    <string name="permission_group_messaging_description">SMS, MMS et plus. Tout ce qui concerne la messagerie.</string>
    <string name="permission_group_connectivity_label">Connectivité</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth et plus. Autorisations liées à la communication avec le monde extérieur.</string>
    <string name="permission_group_sensors_label">Capteurs</string>
    <string name="permission_group_sensors_description">Accéléromètre, gyroscope, capteurs corporels, reconnaissance d\'activité et plus. Façons dont votre appareil peut capturer des entrées.</string>
