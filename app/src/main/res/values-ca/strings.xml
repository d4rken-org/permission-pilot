    <string name="permission_nfc_label">NFC</string>
    <string name="permission_nfc_description">Permet a les aplicacions comunicar-se amb etiquetes, targetes i lectors de Near Field Communication (NFC).</string>
    <string name="permission_manage_media_label">Gestionar mitjans</string>
    <string name="permission_manage_media_description">Permet a les aplicacions modificar o suprimir fitxers de mitjans creats amb altres aplicacions SENSE preguntar-te. Les aplicacions han de tenir permís per accedir a fitxers i mitjans.</string>
    <string name="permission_usage_data_access_label">Accés a dades d\'ús</string>
    <string name="permission_usage_data_access_description">Permet a les aplicacions fer un seguiment de quines altres aplicacions uses i amb quina freqüència, identificar el teu proveïdor de serveis, configuració d\'idioma i altres dades d\'ús.</string>
    <string name="permission_modify_system_settings_label">Modificar configuració del sistema</string>
    <string name="permission_modify_system_settings_description">Permet a les aplicacions modificar les dades de configuració del sistema. Les aplicacions malicioses poden corrompre la configuració del teu sistema.</string>
    <string name="permission_bluetooth_advertise_label">Anunciar Bluetooth</string>
    <string name="permission_bluetooth_advertise_description">Permet a les aplicacions anunciar-se a dispositius Bluetooth propers.</string>
    <string name="permission_get_accounts_label">Obtenir comptes</string>
    <string name="permission_get_accounts_description">Permet a les aplicacions obtenir la llista de comptes coneguts pel telèfon. Això pot incloure qualsevol compte creat per aplicacions que hagis instal·lat.</string>
    <string name="permission_access_background_location_label">Accés a la ubicació en segon pla</string>
    <string name="permission_access_background_location_description">Permet a les aplicacions accedir a la ubicació en qualsevol moment, fins i tot quan no s\'estan utilitzant.</string>
    <string name="permission_access_media_location_label">Accés a la ubicació dels mitjans</string>
    <string name="permission_access_media_location_description">Permet a les aplicacions llegir la ubicació de la teva col·lecció de mitjans.</string>
    <string name="permission_manage_accounts_label">Gestionar comptes</string>
    <string name="permission_manage_accounts_description">Permet a les aplicacions gestionar comptes al dispositiu.</string>
    <string name="permission_wifi_control_label">Control WiFi</string>
    <string name="permission_wifi_control_description">Permet a les aplicacions activar o desactivar WiFi, escanejar i connectar-se a xarxes WiFi, afegir o eliminar xarxes, o iniciar un punt d\'accés només local.</string>
    <string name="permission_change_network_state_label">Canviar estat de la xarxa</string>
    <string name="permission_change_network_state_description">Permet a les aplicacions canviar l\'estat de la connectivitat de xarxa.</string>
    <string name="permission_read_sync_settings_label">Llegir configuració de sincronització</string>
    <string name="permission_read_sync_settings_description">Permet a les aplicacions llegir la configuració de sincronització d\'un compte.</string>
    <string name="permission_install_unknown_apps_label">Instal·lar aplicacions desconegudes (Orígens desconeguts)</string>
    <string name="permission_install_unknown_apps_description">Permet a les aplicacions instal·lar (carregant externament) altres aplicacions al teu dispositiu. Això pot posar en risc el teu dispositiu i les teves dades.</string>
    <string name="permission_all_files_access_label">Accés a tots els fitxers</string>
    <string name="permission_all_files_access_description">Permet a les aplicacions llegir, modificar i suprimir tots els fitxers del teu dispositiu, així com qualsevol dispositiu d\'emmagatzematge connectat. Les aplicacions amb aquest permís també poden accedir a fitxers que no estan relacionats amb l\'aplicació, SENSE notificar-te.</string>
    <string name="permission_appear_on_top_label">Aparèixer a sobre</string>
    <string name="permission_appear_on_top_description">Permet a les aplicacions mostrar coses a sobre d\'altres aplicacions que estàs utilitzant (incloent mostrar publicitat). Això pot interferir amb l\'ús d\'altres aplicacions.</string>
    <string name="permission_files_and_media_label">Fitxers i mitjans</string>
    <string name="permission_files_and_media_description">Permet a les aplicacions accedir a fitxers i mitjans del teu dispositiu.</string>
    <string name="permission_access_to_media_only_label">Accés només a mitjans</string>
    <string name="permission_access_to_media_only_description">Permet a les aplicacions accedir només als mitjans del teu dispositiu.</string>
    <string name="permission_access_precise_location_label">Accés a la ubicació precisa/exacta</string>
    <string name="permission_access_precise_location_description">Permet a les aplicacions accedir a la ubicació precisa/exacta del teu dispositiu. Això NO es recomana per a la majoria d\'aplicacions (excepte per a algunes aplicacions com mapes, serveis de taxi, serveis de seguiment de dispositius com Find My Device, etc.).</string>
    <string name="permission_access_approximate_location_label">Accés a la ubicació aproximada</string>
    <string name="permission_access_approximate_location_description">Permet a les aplicacions accedir a la ubicació aproximada del teu dispositiu. Aquest permís és suficient per a la majoria d\'aplicacions que necessiten accés a la teva ubicació per a la funcionalitat principal.</string>
    <string name="permission_ignore_battery_optimizations_label">Ignorar optimitzacions de bateria</string>
    <string name="permission_ignore_battery_optimizations_description">Permet a les aplicacions funcionar sense obstacles en segon pla. Això pot esgotar la bateria. Es recomana només per a aplicacions que fan còpies de seguretat i/o sincronitzacions contínues/freqüents (p. ex. Google Photos), aplicacions molt utilitzades freqüentment (p. ex. correu electrònic, xat, SMS, etc.), així com per a aquelles aplicacions que tenen una notificació persistent.</string>
    <string name="permission_notification_access_label">Accés a notificacions</string>
    <string name="permission_notification_access_description">Permet a les aplicacions llegir totes les teves notificacions, incloent informació personal (com noms de contactes i text de missatges que reps), posposar o descartar notificacions o activar botons d\'acció que contenen (com respondre trucades telefòniques, suprimir el teu SMS/correu electrònic, etc.) i activar o desactivar No molestar (DND) i canviar la configuració relacionada.</string>
    <string name="permission_do_not_disturb_permission_label">Permís de No molestar (DND)</string>
    <string name="permission_do_not_disturb_permission_description">Permet a les aplicacions activar o desactivar No molestar (DND) i canviar la configuració relacionada.</string>
    <string name="permission_alarms_and_reminders_label">Alarmes i recordatoris</string>
    <string name="permission_alarms_and_reminders_description">Permet a les aplicacions configurar alarmes i programar accions sensibles al temps. Això permet a les aplicacions funcionar en segon pla, la qual cosa pot utilitzar més bateria. Desactivar aquest permís per a una aplicació faria que les alarmes existents i els esdeveniments basats en temps programats per aquesta aplicació particular deixessin de funcionar.</string>
    <string name="permission_premium_sms_services_label">Accés a SMS Premium</string>
    <string name="permission_premium_sms_services_description">Permet a les aplicacions utilitzar serveis de missatges de text premium. Això et costarà diners.</string>
    <string name="permission_unlimited_data_access_label">Accés il·limitat a dades</string>
    <string name="permission_unlimited_data_access_description">Permet a les aplicacions accedir a dades sense restricció. Les aplicacions poden consumir dades fins i tot quan l\'Estalviador de dades està activat.</string>
    <string name="permission_group_calendar_label">Calendari</string>
    <string name="permission_group_calendar_description"><![CDATA[Permisos relacionats amb el calendari, p. ex. veure i editar entrades.]]></string>
    <string name="permission_group_files_label">Fitxers i mitjans</string>
    <string name="permission_group_files_description">Permisos relacionats amb fitxers. Accés a emmagatzematge, documents i mitjans.</string>
    <string name="permission_group_other_label">Altres</string>
    <string name="permission_group_other_description">Altres permisos sense categoria específica.</string>
    <string name="permission_group_messaging_label">Missatgeria</string>
    <string name="permission_group_messaging_description">SMS, MMS i més. Tot relacionat amb la missatgeria.</string>
    <string name="permission_group_connectivity_label">Connectivitat</string>
    <string name="permission_group_connectivity_description">WiFi, LTE, Bluetooth i més. Permisos relacionats amb la comunicació amb el món exterior.</string>
    <string name="permission_group_sensors_label">Sensors</string>
    <string name="permission_group_sensors_description">Acceleròmetre, giroscopi, sensors corporals, reconeixement d\'activitat i més. Maneres en què el teu dispositiu pot capturar entrada.</string>
    <string name="permission_group_calls_label">Trucades</string>
    <string name="permission_group_calls_description">Permisos relacionats amb trucades</string>
    <string name="permission_group_location_label">Ubicació</string>
